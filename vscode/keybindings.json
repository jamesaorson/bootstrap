[
    {
        "key": "ctrl+o",
        "command": "workbench.action.switchWindow"
    },
    {
        "key": "ctrl+w",
        "command": "-workbench.action.switchWindow"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.action.terminal.runSelectedText",
        "when": "terminalProcessSupported"
    },
    {
        "key": "alt+e",
        "command": "cursorLineEnd",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+e",
        "command": "-cursorLineEnd",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+x",
        "command": "editor.action.selectToBracket"
    }
]