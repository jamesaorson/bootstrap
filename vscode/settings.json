{
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "FiraCode Nerd Font Mono",
  "editor.renderWhitespace": "all",
  "workbench.iconTheme": "material-icon-theme",
  "yaml.schemas": {
    "file:///home/jamesaorson/.vscode/extensions/docsmsft.docs-yaml-1.0.5/dist/toc.schema.json": "/toc\\.yml/i"
  },
  "redhat.telemetry.enabled": false,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "python": true,
    "Log": false,
    "jsonc": true
  },
  "git.ignoreMissingGitWarning": true,
  "cSpell.userWords": [
    "agentpool",
    "azuread",
    "azurerm",
    "eastus",
    "snet",
    "vnet"
],
  "FSharp.suggestGitignore": false,
  "git.openRepositoryInParentFolders": "always",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark Default",
  "files.associations": {
    "*.fs": "fsharp",
    "*.fsproj": "xml"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "githubPullRequests.pullBranch": "never",
  "workbench.colorTheme": "GitHub Dark Default",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "database-client.autoSync": true,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "makefile.configureOnOpen": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "github.copilot.selectedCompletionModel": "o3-mini",
  "editor.formatOnSave": true,
  "security.workspace.trust.untrustedFiles": "open",
  "github.copilot.nextEditSuggestions.enabled": false,
  "editor.defaultFormatter": "ms-python.black-formatter",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "go.toolsManagement.autoUpdate": true,
  "git.alwaysSignOff": true,
  "docker.extension.enableComposeLanguageServer": false
}