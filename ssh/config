# Hosts
Host git.sr.ht
    ControlMaster auto
	ControlPath ~/.ssh/%r%h:%p
	ServerAliveCountMax=30
	ServerAliveInterval=5
    User git
    IdentityFile ~/.ssh/id_sourcehut
